:root,page,body,html,#app,.uni-app{--themeColor: #1ABDF2;--primary-accent: #1ABDF2;--globalcolor: #1a1b1c;--focusColor: #4c4d4e;--descColor: #7e8081;--greyColor: #8e8e8e;--subColor: #b1b2b3;--lightColor: #e4e4e4;--globalBgColor1: #e3e4e5;--globalBgColor2: #f6f7f8;--globalBgColor3: #ffffff;--bg-light: #f7f8fa;--blue-accent: #3978F7;--text-main: #333333;--text-sub: #666666;--line-gray: #E5E5E5;--bg-white: #FFFFFF}.header-emptybox{width:100%;height:230rpx;position:relative}.profile-title{position:absolute;font-size:55rpx;font-weight:580;color:#1c1c1e;font-family:Helvetica Neue,PingFang SC,Helvetica,Arial,sans-serif;top:130rpx;left:35rpx}.nav-scroll{background:#fff;padding:12rpx 0;box-shadow:0 1px 2px rgba(0,0,0,.1);white-space:nowrap;position:relative;z-index:10;overflow:hidden}.nav-scroll::-webkit-scrollbar{display:none}.nav-scroll .nav-container{display:flex;padding:0 32rpx;position:relative}.nav-scroll .nav-item{flex-shrink:0;padding:16rpx 0;margin:0 12rpx;font-size:28rpx;color:#5f6368;border-radius:999rpx;transition:all .3s cubic-bezier(.4,0,.2,1);transform-origin:center;position:relative;z-index:1}.nav-scroll .nav-item-inner{padding:16rpx 32rpx;transition:all .3s ease;display:block}.nav-scroll .nav-item.active{color:#4285f4;font-weight:500}.nav-scroll .nav-item.active .nav-item-inner{position:relative;z-index:2}.nav-scroll .nav-item.active:before{content:"";position:absolute;top:50%;left:50%;width:100%;height:100%;background:rgba(66,133,244,.1);border-radius:999rpx;transform:translate(-50%,-50%) scale(1);z-index:1;transition:all .3s cubic-bezier(.4,0,.2,1)}.nav-scroll .nav-item.animate-active{transform:scale(1.05)}.nav-scroll .nav-item.animate-active:before{transform:translate(-50%,-50%) scale(1.15);opacity:.8}.nav-scroll .nav-item:not(.active):hover,.nav-scroll .nav-item:not(.active):active{transform:scale(.95);opacity:.9}.list-container{padding:32rpx;background:#f8f9fa;transition:opacity .3s ease}.list-container.fade{opacity:0}.service-card{position:relative;background:#fff;border-radius:24rpx;margin-bottom:32rpx;padding:24rpx;display:flex;box-shadow:0 4rpx 8rpx rgba(0,0,0,.1);transition:all .2s cubic-bezier(.4,0,.2,1)}.service-card:active{transform:scale(.98);box-shadow:0 8rpx 16rpx rgba(0,0,0,.15)}.service-card .top-right-container{position:absolute;top:24rpx;right:24rpx;display:flex;flex-direction:column;align-items:flex-end;z-index:2}.service-card .top-right-container .service-area{font-size:28rpx;color:#5f6368;margin-bottom:8rpx}.service-card .top-right-container .distance-container{background:rgba(66,133,244,.1);padding:8rpx 16rpx;border-radius:16rpx;transition:opacity .2s}.service-card .top-right-container .distance-container .distance-text{color:#4285f4;font-size:24rpx;font-weight:500}.service-card .top-right-container .distance-container.hidden{opacity:0;pointer-events:none}.service-card .left-column{width:160rpx;margin-right:24rpx;flex-shrink:0}.service-card .left-column .img-container{width:160rpx;height:160rpx;border-radius:50%;overflow:hidden;transition:transform .3s}.service-card .left-column .img-container .service-img{width:100%;height:100%;object-fit:cover;transition:transform .3s}.service-card .content-wrapper{flex:1;min-width:0;padding-right:120rpx}.service-card .content-wrapper .title-row{margin-bottom:16rpx}.service-card .content-wrapper .title-row .service-title{font-size:34rpx;color:#202124;font-weight:600;line-height:1.4;transition:color .2s}.service-card .content-wrapper .info-section{margin-bottom:16rpx}.service-card .content-wrapper .info-section .info-item{display:flex;margin-bottom:8rpx}.service-card .content-wrapper .info-section .info-item .info-value{font-size:28rpx;color:#202124;overflow:hidden;text-overflow:ellipsis}.service-card .content-wrapper .info-section .info-item .info-value.desc-text{min-height:30px;line-height:30px;display:-webkit-box;-webkit-box-orient:vertical;-webkit-line-clamp:2;overflow:hidden;text-overflow:ellipsis;line-height:1.5;white-space:normal;overflow-wrap:break-word;word-break:keep-all}.service-card .content-wrapper .skills-container{margin-top:16rpx;display:flex;flex-wrap:wrap;gap:12rpx;overflow:hidden;padding-bottom:8rpx;margin-left:-170rpx;width:calc(100% + 270rpx)}.service-card .content-wrapper .skills-container .skill-tag{flex-shrink:0;background:#f1f3f4;color:#3c4043;padding:8rpx 16rpx;border-radius:20rpx;font-size:24rpx;transition:all .2s}.service-card:hover .service-title{color:#4285f4}.service-card:hover .img-container{transform:scale(1.05)}.skeleton{padding:32rpx}.skeleton .skeleton-card{background:#fff;border-radius:24rpx;margin-bottom:32rpx;padding:24rpx;display:flex;align-items:center;animation:skeleton-pulse 1.5s infinite}.skeleton .skeleton-card .skeleton-avatar{width:160rpx;height:160rpx;border-radius:50%;background:#e0e0e0;margin-right:24rpx}.skeleton .skeleton-card .skeleton-content{flex:1}.skeleton .skeleton-card .skeleton-content .skeleton-title{width:80%;height:34rpx;background:#e0e0e0;margin-bottom:16rpx;border-radius:8rpx}.skeleton .skeleton-card .skeleton-content .skeleton-desc{width:60%;height:28rpx;background:#e0e0e0;border-radius:8rpx}@keyframes skeleton-pulse{0%,to{opacity:1}50%{opacity:.5}}.no-data{text-align:center;color:#5f6368;font-size:28rpx;padding:32rpx}
