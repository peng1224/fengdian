:root,page,body,html,#app,.uni-app{--themeColor: #1ABDF2;--primary-accent: #1ABDF2;--globalcolor: #1a1b1c;--focusColor: #4c4d4e;--descColor: #7e8081;--greyColor: #8e8e8e;--subColor: #b1b2b3;--lightColor: #e4e4e4;--globalBgColor1: #e3e4e5;--globalBgColor2: #f6f7f8;--globalBgColor3: #ffffff;--bg-light: #f7f8fa;--blue-accent: #3978F7;--text-main: #333333;--text-sub: #666666;--line-gray: #E5E5E5;--bg-white: #FFFFFF}.custom-navbar{position:fixed;top:0;left:0;right:0;height:100rpx;padding-top:50rpx;background:linear-gradient(180deg,#fff,#f8f8f8);display:flex;align-items:center;justify-content:space-between;padding-left:20rpx;padding-right:20rpx;box-shadow:0 4rpx 12rpx rgba(0,0,0,.05);z-index:1000;transition:all .3s ease}.back-icon{font-size:40rpx;color:#333;width:80rpx;height:80rpx;display:flex;align-items:center;justify-content:center;border-radius:50%;transition:background-color .2s ease}.back-icon:hover{background-color:rgba(0,0,0,.05)}.navbar-title{position:absolute;left:50%;transform:translate(-50%);font-size:34rpx;font-weight:700;color:#222;text-align:center;white-space:nowrap}.terms-container{padding:163rpx 24rpx 40rpx;background-color:#f5f5f5;min-height:100vh}.section{margin-bottom:50rpx}.section-title{font-size:32rpx;font-weight:700;color:#4285f4;margin-bottom:20rpx}.subsection{margin-top:30rpx}.sub-title{font-size:30rpx;font-weight:600;color:#4285f4;margin-bottom:15rpx}.sub-content{padding-left:12rpx;padding:30rpx}.section-content{background-color:#fff;border-radius:20rpx;padding:30rpx;box-shadow:0 2rpx 8rpx rgba(0,0,0,.03)}.item{margin-bottom:20rpx}.item-text{font-size:28rpx;line-height:1.8;color:#333}.indent{padding-left:24rpx;display:block}.contact-us{margin-top:50rpx;padding:24rpx;background-color:#fff;border-radius:20rpx;text-align:center;box-shadow:0 2rpx 8rpx rgba(0,0,0,.03)}.email{font-size:28rpx;color:#4285f4;margin-top:12rpx;display:block}
